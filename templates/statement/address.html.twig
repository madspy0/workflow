<div class="app-card app-card-update shadow-sm">
    <div class="app-card-header p-3">
        <div class="row justify-content-between align-items-center">
            <div class="col-auto">
                <h4 class="app-card-title">Заявник</h4>
            </div><!--//col-->
        </div><!--//row-->
    </div><!--//app-card-header-->
    <div class="app-card-body p-3 p-lg-4">
        <div>
            <div>{{ developmentApplication.applicantLastname }} {{ developmentApplication.applicantFirstname }} {{ developmentApplication.applicantMiddlename }}</div>
            <div><small>Тел: </small> {{ developmentApplication.phoneNumber }}</div>
            <div>{{ developmentApplication.email }}</div>
            <div>{{ developmentApplication.applicantStreetAddress }} {{ developmentApplication.applicantBuild }}</div>
            <div>{{ developmentApplication.city }} {{ developmentApplication.region }}</div>
            <div>{{ developmentApplication.country.titleUa }}</div>
            </div>
    </div><!--//app-card-body-->
</div><!--//app-card-->
<div class="app-card shadow-sm app-card-update">
    <div class="app-card-header p-3">
        <div class="row justify-content-between align-items-center">
            <div class="col-auto">
                <h4 class="app-card-title">Ділянка</h4>
            </div><!--//col-->
        </div><!--//row-->
    </div><!--//app-card-header-->
    <div class="app-card-body p-3 p-lg-4">
        <div>
            <div>{{ developmentApplication.landAddress }}</div>
            <div>{{ developmentApplication.landCity }}</div>
            <div id="map" class="map" data-geom="{{ developmentApplication.geom }}"></div>
        </div>
    </div><!--//app-card-body-->
</div><!--//app-card-->
